{% extends 'office/base.html.twig' %}

{% block contenu %}

    <h3>Ajout d'un élément</h3>

    {{ form_start(form, { 'attr' : { 'id' : 'project-form' }}) }}
        {{ form_row(form.title, { 'attr' : { 'class' : 'form-control' } }) }}
        {{ form_row(form.period, { 'attr' : { 'class' : 'form-control' } }) }}
        {{ form_row(form.description, { 'attr' : { 'class' : 'form-control' } }) }}

    <button class="btn btn-success center-button mt-2" type="submit">
        <span class="oi oi-plus" aria-hidden="true"></span> Ajouter un élément
    </button>
    {{ form_end(form) }}

{% endblock %}
