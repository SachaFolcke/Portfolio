{% extends 'office/base.html.twig' %}

{% block contenu %}

    <h3>Modifier un projet</h3>

    {{ form_start(form, { 'attr' : { 'id' : 'project-form' }}) }}
    {{ form_row(form.titre, { 'attr' : { 'class' : 'form-control' } }) }}
    {{ form_row(form.catchPhrase, { 'attr' : { 'class' : 'form-control' } }) }}
    {{ form_row(form.periode, { 'attr' : { 'class' : 'form-control' } }) }}
    {{ form_row(form.compoGroupe, { 'attr' : { 'class' : 'form-control' } }) }}
    {{ form_row(form.langages, { 'attr' : { 'class' : 'form-control' } }) }}
    {{ form_row(form.description, { 'attr' : { 'class' : 'form-control' } }) }}
    <div class="checkbox-inline">
        {{ form_row(form.online) }}
    </div>

    <button class="center-button btn btn-warning" type="submit">
        <span class="oi oi-pencil" title="pencil" aria-hidden="true"></span> Modifier le projet !
    </button>
    {{ form_end(form) }}

{% endblock %}
